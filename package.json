{
  "name": "kids-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "docs": "npx typedoc",
    "start": "nest start",
    "prestart:dev": "docker-compose up -d",
    "start:dev": "nest start --watch",
    "poststart:dev": "docker-compose down",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "pretty": "npx prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --watch --detectOpenHandles --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.12",
    "@nestjs/core": "^7.0.0",
    "@nestjs/graphql": "^7.7.0",
    "@nestjs/platform-express": "^7.0.0",
    "@types/lodash": "^4.14.162",
    "apollo-server-express": "^2.18.2",
    "crc-32": "^1.2.0",
    "dotenv": "^8.2.0",
    "graphql": "^15.3.0",
    "graphql-tools": "^6.2.4",
    "helmet": "^4.4.1",
    "ioredis": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "short-uuid": "^4.1.0",
    "user-agent-parse": "^0.1.12",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.17.10",
    "@graphql-codegen/introspection": "1.18.0",
    "@graphql-codegen/typescript": "1.17.10",
    "@graphql-codegen/typescript-resolvers": "1.17.10",
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@prisma/cli": "^2.13.0",
    "@prisma/client": "^2.13.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.3",
    "@types/helmet": "^4.0.0",
    "@types/ioredis": "^4.17.5",
    "@types/jest": "26.0.10",
    "@types/node": "^13.9.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.4.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.19.2",
    "typedoc-neo-theme": "^1.0.10",
    "typedoc-plugin-markdown": "^3.0.11",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "resolutions": {
    "fs-capacitor": "5.0.0",
    "graphql-upload": "11.0.0"
  }
}
